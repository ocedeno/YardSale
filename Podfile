# platform :ios, '9.0'

target 'Yard Sale' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    pod 'FirebaseUI/Storage'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'Bolts'
    pod 'GoogleSignIn'
    
    # Pods for Yard Sale
    
    target 'Yard SaleTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'Yard SaleUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
